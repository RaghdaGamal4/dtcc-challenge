{
    "name": "dtcc-challenge",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --loglevel warn --write",
        "preview": "vite preview",
        "build:dev": "vite build --mode development",
        "build:test": "vite build --mode testing",
        "build:stg": "vite build --mode staging",
        "build:prod": "vite build --mode production",
        "build-all": "concurrently \"npm run build:dev\" \"npm run build:test\" \"npm run build:stg\" \"npm run build:prod\"",
        "test": "jest --colors --coverage --runInBand --silent",
        "build:docker": "docker build -t dtcc-challenge .",
        "start:docker": "docker run -p 3000:80 dtcc-challenge"
    },
    "dependencies": {
        "@mui/icons-material": "^5.15.1",
        "@mui/lab": "^5.0.0-alpha.166",
        "@mui/material": "^5.14.15",
        "@mui/x-date-pickers": "^6.19.6",
        "@rainbow-me/rainbowkit": "^2.0.2",
        "@reduxjs/toolkit": "^2.0.1",
        "graphql": "^16.8.1",
        "graphql-request": "^6.1.0",
        "i18next": "^23.7.11",
        "i18next-browser-languagedetector": "^7.2.0",
        "jest-environment-jsdom": "^29.7.0",
        "lodash": "^4.17.21",
        "lottie-react": "^2.4.0",
        "moment": "^2.30.1",
        "react": "^18.2.0",
        "react-datepicker": "^4.25.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.1",
        "react-i18next": "^13.3.1",
        "react-paginate": "^8.2.0",
        "react-redux": "^9.0.4",
        "react-router-dom": "^6.21.1",
        "react-toastify": "^9.1.3",
        "viem": "^2.8.10",
        "wagmi": "^2.5.7",
        "web3": "^4.6.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.23.7",
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-react": "^7.23.3",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/react": "^14.1.2",
        "@types/jest": "^29.5.11",
        "@types/react": "^18.2.43",
        "@types/react-datepicker": "^6.0.3",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "autoprefixer": "^10.4.16",
        "babel-jest": "^29.7.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "postcss": "^8.4.32",
        "prettier": "^3.1.1",
        "prettier-plugin-tailwindcss": "^0.5.9",
        "tailwindcss": "^3.4.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.8"
    }
}
