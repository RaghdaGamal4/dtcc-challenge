{
    "singleQuote": false,
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "endOfLine": "lf",
    "semi": true,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "plugins": ["prettier-plugin-tailwindcss"]
}
